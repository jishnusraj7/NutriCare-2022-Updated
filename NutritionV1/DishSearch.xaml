<Page x:Class="NutritionV1.DishSearch"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dg="http://schemas.microsoft.com/wpf/2008/toolkit"
    xmlns:Nutrition="clr-namespace:NutritionV1"
    Title="Dish Search" Loaded="Page_Loaded" >
    
    <Page.Resources>
        <Storyboard x:Key="MouseOver" x:Name="MouseEnters">
            <DoubleAnimationUsingKeyFrames  BeginTime="00:00:00"  Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" >
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="1"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="1.266"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00"  Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="1"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="1.266"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00"  Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="-0.7"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00"  Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="-0.112"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="MouseLeave" x:Name="MouseLeaves">
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00"  Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="1"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.6000000" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00"  Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="1"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.6000000" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.6000000" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00"  Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.6000000" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="FlipBoard">
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="pageFlip" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="1"/>
                <SplineDoubleKeyFrame KeyTime="00:00:01.5000000" Value="10.538"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="pageFlip" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <SplineDoubleKeyFrame KeyTime="00:00:01.5000000" Value="-127"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="pageFlip" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="1"/>
                <SplineDoubleKeyFrame KeyTime="00:00:01.5000000" Value="10.6"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="pageFlip" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <SplineDoubleKeyFrame KeyTime="00:00:01.5000000" Value="-120"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="pageFlip" Storyboard.TargetProperty="(UIElement.Opacity)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="1"/>
                <SplineDoubleKeyFrame KeyTime="00:00:01.5000000" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
            <ObjectAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="pageFlip" Storyboard.TargetProperty="(UIElement.Visibility)">
                <DiscreteObjectKeyFrame KeyTime="00:00:00" Value="{x:Static Visibility.Visible}"/>
                <DiscreteObjectKeyFrame KeyTime="00:00:01.5000000" Value="{x:Static Visibility.Hidden}"/>
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="FadeIn">
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="MainContent" Storyboard.TargetProperty="(UIElement.Opacity)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <SplineDoubleKeyFrame KeyTime="00:00:02" Value="1"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="FadeOut">
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="MainContent" Storyboard.TargetProperty="(UIElement.Opacity)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="1"/>
                <SplineDoubleKeyFrame KeyTime="00:00:02" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="ExpandMyGrid">
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="MyGrid" Storyboard.TargetProperty="(FrameworkElement.Width)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="980"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="CollapseMyGrid">
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="MyGrid" Storyboard.TargetProperty="(FrameworkElement.Width)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="980"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="ExpandYourGrid">
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="YourGrid" Storyboard.TargetProperty="(FrameworkElement.Width)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="680"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="CollapseYourGrid">
            <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Storyboard.TargetName="YourGrid" Storyboard.TargetProperty="(FrameworkElement.Width)">
                <SplineDoubleKeyFrame KeyTime="00:00:00" Value="680"/>
                <SplineDoubleKeyFrame KeyTime="00:00:00.5000000" Value="0"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>

        <Style TargetType="{x:Type ListBox}">
            <Setter Property="ItemTemplate">
                <Setter.Value>
                    <DataTemplate>
                        <Border BorderBrush="DarkGray" BorderThickness="1" Margin="10" Background="White">
                            <Border.BitmapEffect>
                                <DropShadowBitmapEffect Color="DarkGray" Direction="315" ShadowDepth="5" Softness="0.25" Opacity="0.5"/>
                            </Border.BitmapEffect>
                            <StackPanel Margin="5" Cursor="Hand" x:Name="spDish" MouseDown="spDish_MouseDown"  ToolTip="{Binding Path=DisplayName}">
                                <Image Name="Image" Source="{Binding Path=DisplayImage}" Stretch="Fill" Width="100" Height="100" />
                                <TextBlock Foreground="Black" FontSize="11" Name="Name" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="2" Text="{Binding Path=IconName}" FontWeight="Bold"/>
                                <Label Name="ID" Style="{DynamicResource LabelStyle}" HorizontalAlignment="Left" VerticalAlignment="Center" Visibility="Hidden" Content="{Binding Path=Id}"/>
                            </StackPanel>
                        </Border>                                               
                    </DataTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ItemsPanel">
                <Setter.Value>
                    <ItemsPanelTemplate>
                        <WrapPanel />
                    </ItemsPanelTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled" />
        </Style>

        <DataTemplate x:Key="NameDisplayTemplate">
            <TextBlock Name="txtIngredientName" Height="20" Width="100" FontSize="12" FontWeight="Normal" Text="{Binding IngredientName}">
                <TextBlock.ToolTip> 
                    <ToolTip Content="{Binding IngredientName}" /> 
                </TextBlock.ToolTip>
            </TextBlock>
        </DataTemplate>
        
        <!--<DataTemplate x:Key="NameTemplate">
            <TextBlock Name="txtDishName" ToolTip="{Binding Name}" Height="20" FontSize="12" FontWeight="Normal" Text="{Binding Name}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
        </DataTemplate>
        <DataTemplate x:Key="DisplayNameTemplate">
            <TextBlock Name="txtDisplayName" ToolTip="{Binding DisplayName}" Height="20" FontSize="12" FontWeight="Normal" Text="{Binding DisplayName}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
        </DataTemplate>-->

        <DataTemplate x:Key="idTemplate">
            <Label Name="lblID" FontSize="12" FontWeight="Normal" Content="{Binding NutrientID}" />
        </DataTemplate>
        <DataTemplate x:Key="NutrientParamTemplate">
            <TextBlock Name="txtNParam" Height="20" Width="Auto" Text="{Binding NutrientParam}" Margin="5,0" />
        </DataTemplate>
        <DataTemplate x:Key="nutrientvalueTemplate">
            <TextBlock Name="txtNutriValue" Width="75" FontSize="12" FontWeight="Normal" Text="{Binding NutrientValue}"  Margin="5,0" />
        </DataTemplate>
        <DataTemplate x:Key="aminovalueTemplate">
            <TextBlock Name="txtAminoValue" Width="75" FontSize="12" FontWeight="Normal" Text="{Binding NutrientValue}"  Margin="5,0"/>
        </DataTemplate>
        <DataTemplate x:Key="fattyvalueTemplate">
            <TextBlock Name="txtFattyValue" Width="75" FontSize="12" FontWeight="Normal" Text="{Binding NutrientValue}"  Margin="5,0"/>
        </DataTemplate>
        <DataTemplate x:Key="unitTemplate">
            <TextBlock Text="{Binding NutrientUnit}" Height="20" Margin="5,0"/>
        </DataTemplate>

        <DataTemplate x:Key="QuantityTemplate">
            <TextBlock Name="txtQuantity" Width="25" FontSize="12" FontWeight="Normal" Text="{Binding Quantity}"  Margin="5,0" TextAlignment="Right"/>
        </DataTemplate>

        <DataTemplate x:Name="UnitsTemplate" x:Key="UnitsTemplate">
            <TextBlock Name="txtUnit" Width="55" FontSize="12" FontWeight="Normal" Text="{Binding StandardUnitName}"  Margin="5,0"/>
        </DataTemplate>

        <DataTemplate x:Key="MacroTemplate">
            <Grid Margin="50,0,0,0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="5" />
                    <RowDefinition Height="20" />
                    <RowDefinition Height="20" />
                    <RowDefinition Height="20" />
                    <RowDefinition Height="20" />
                    <RowDefinition Height="20" />
                    <RowDefinition Height="20" />
                    <RowDefinition Height="5" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="130"/>
                    <ColumnDefinition Width="30"/>
                    <ColumnDefinition Width="30"/>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="1" Grid.Column="0" Text="Calorie" x:Name="lblDataCalorie" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding Calorie}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="1" Grid.Column="2" Text="Kcal" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="2" Grid.Column="0" Text="Protein" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="2" Grid.Column="1" Text="{Binding Protien}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="2" Grid.Column="2" Text="gm" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="3" Grid.Column="0" Text="CarboHydrates" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="3" Grid.Column="1" Text="{Binding CarboHydrate}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="3" Grid.Column="2" Text="gm" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="4" Grid.Column="0" Text="FAT" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="4" Grid.Column="1" Text="{Binding Fat}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="4" Grid.Column="2" Text="gm" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="5" Grid.Column="0" Text="Fibre" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="5" Grid.Column="1" Text="{Binding Fibre}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="5" Grid.Column="2" Text="gm" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="6" Grid.Column="0" Text="Moisture" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="6" Grid.Column="1" Text="{Binding Moisture}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="6" Grid.Column="2" Text="gm" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="MineralsTemplate">
            <Grid Margin="50,0,0,0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="5" />
                    <RowDefinition Height="20" />
                    <RowDefinition Height="20" />
                    <RowDefinition Height="20" />
                    <RowDefinition Height="20" />
                    <RowDefinition Height="20" />
                    <RowDefinition Height="20" />
                    <RowDefinition Height="5" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="130"/>
                    <ColumnDefinition Width="30"/>
                    <ColumnDefinition Width="30"/>
                </Grid.ColumnDefinitions>
                <TextBlock Grid.Row="1" Grid.Column="0" Text="Iron" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="1" Grid.Column="1" Text="{Binding Iron}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="1" Grid.Column="2" Text="mg" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="2" Grid.Column="0" Text="Calcium" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
