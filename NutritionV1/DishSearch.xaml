                <TextBlock Grid.Row="2" Grid.Column="2" Text="mg" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="3" Grid.Column="0" Text="Phosphorus" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="3" Grid.Column="1" Text="{Binding Phosphorus}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="3" Grid.Column="2" Text="mg" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="4" Grid.Column="0" Text="Sodium" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="4" Grid.Column="1" Text="{Binding Sodium}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="4" Grid.Column="2" Text="mg" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="5" Grid.Column="0" Text="Pottasium" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="5" Grid.Column="1" Text="{Binding Pottasium}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="5" Grid.Column="2" Text="mg" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="6" Grid.Column="0" Text="Zinc" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="6" Grid.Column="1" Text="{Binding Zinc}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="6" Grid.Column="2" Text="mg" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
