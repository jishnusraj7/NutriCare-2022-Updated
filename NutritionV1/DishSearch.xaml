                <TextBlock Grid.Row="3" Grid.Column="1" Text="{Binding NicotinicAcid}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="3" Grid.Column="2" Text="mg" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="4" Grid.Column="0" Text="Pyridoxine" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="4" Grid.Column="1" Text="{Binding Pyridoxine}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="4" Grid.Column="2" Text="mg" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="5" Grid.Column="0" Text="Folic Acid" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="5" Grid.Column="1" Text="{Binding FolicAcid}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="5" Grid.Column="2" Text="mcg" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="6" Grid.Column="0" Text="VitaminC" TextAlignment="Left" Width="150" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="6" Grid.Column="1" Text="{Binding VitaminC}" TextAlignment="Left" Width="70" FontSize="11" Margin="0,0,0,0"/>
                <TextBlock Grid.Row="6" Grid.Column="2" Text="mg" TextAlignment="Left" Width="30" FontSize="11" Margin="0,0,0,0"/>
            </Grid>
        </DataTemplate>
       
        <DataTemplate x:Key="NameTemplate">
            <Grid Width="100" Height="120" Margin="7,7,0,0" >
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition Height="30" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Rectangle Name="RecImage" Style="{DynamicResource ImageDisplay}" Grid.Column="0" Grid.Row="0" RadiusX="15.5" RadiusY="15.5"   VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0,0,0,0" ToolTip="{Binding Path=Name}" />
                    <Image Grid.Column="0" Grid.Row="0" Margin="3,3,3,3" Source="{Binding Path=DisplayImage}" ToolTip="{Binding Path=Name}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Height="80" Width="90" ></Image>
                    <Label Foreground="White" FontSize="11" Grid.Column="0" Grid.Row="0" Margin="0,0,0,0" Content="{Binding Path=Name}" ToolTip="{Binding Path=Name}" HorizontalAlignment="Stretch" VerticalAlignment="Center" HorizontalContentAlignment="Center" Background="Transparent" />
                    <TextBlock TextDecorations="Underline" Foreground="DarkBlue" FontStyle="Italic" FontWeight="Thin" FontSize="11" Grid.Column="0" Grid.Row="1" Margin="0,0,0,0" Text="View More" HorizontalAlignment="Center" VerticalAlignment="Top" Background="Transparent" Cursor="Hand" MouseDown="Image_MouseDown" />
                </Grid>
            </Grid>
        </DataTemplate>
        <DataTemplate x:Key="DisplayNameTemplate">
            <Grid Width="100" Height="120"  Margin="7,7,0,0" >
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition Height="30" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Rectangle Name="RecImage" Style="{DynamicResource ImageDisplay}" Grid.Column="0" Grid.Row="0" RadiusX="15.5" RadiusY="15.5"   VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Margin="0,0,0,0" ToolTip="{Binding Path=DisplayName}" />
                    <Image Grid.Column="0" Grid.Row="0" Margin="3,3,3,3" Source="{Binding Path=DisplayImage}" ToolTip="{Binding Path=DisplayName}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Height="80" Width="90" ></Image>
                    <Label Foreground="White" FontSize="11" Grid.Column="0" Grid.Row="0" Margin="0,0,0,0" Content="{Binding Path=DisplayName}" ToolTip="{Binding Path=DisplayName}" HorizontalAlignment="Stretch" VerticalAlignment="Center" HorizontalContentAlignment="Center" Background="Transparent" />
                    <TextBlock TextDecorations="Underline" Foreground="DarkBlue" FontStyle="Italic" FontWeight="Thin" FontSize="11" Grid.Column="0" Grid.Row="1" Margin="0,0,0,0" Text="View More" HorizontalAlignment="Center" VerticalAlignment="Top" Background="Transparent" Cursor="Hand" MouseDown="Image_MouseDown" />
                </Grid>
            </Grid>
        </DataTemplate>

    </Page.Resources>
    
    <Grid Name="grdIngradientSearch" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
            <Grid.RowDefinitions>
                <RowDefinition Height="110" />
                <RowDefinition Height="0" />
                <RowDefinition />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
            </Grid.ColumnDefinitions>

            <Grid Grid.Column="0" Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
                <Grid.RowDefinitions>
                    <RowDefinition Height="10" />
                    <RowDefinition Height="30" />
                    <RowDefinition Height="30" />
                    <RowDefinition Height="30" />
                    <RowDefinition Height="10" />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="100"/>
                    <ColumnDefinition Width="100"/>
                    <ColumnDefinition Width="200"/>
                    <ColumnDefinition Width="100"/>
                    <ColumnDefinition Width="200"/>
                    <ColumnDefinition />
                </Grid.ColumnDefinitions>

